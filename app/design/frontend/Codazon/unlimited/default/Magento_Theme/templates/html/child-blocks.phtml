<?php
$html = '';
$childBlocks = explode(',', $this->getData('child_blocks'));
foreach ($childBlocks as $childBlockName) {
    $childHtml = $this->getBlockHtml($childBlockName);
    if ($childHtml) {
        $html .= $childHtml;
    }
}
echo $html;
?>