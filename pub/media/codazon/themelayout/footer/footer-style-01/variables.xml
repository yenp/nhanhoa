<?xml version="1.0"?>
<config>
  <identifier>footer-style-01</identifier>
  <title>Footer Style 01</title>
  <variables>
    <footer_background>#ffffff</footer_background>
    <footer_background_file></footer_background_file>
    <footer_background_image_option>0 0 no-repeat fixed</footer_background_image_option>
    <footer_background_normal></footer_background_normal>
    <footer_border>#ffffff</footer_border>
    <footer_bottom_background></footer_bottom_background>
    <footer_font>13px</footer_font>
    <footer_hover_text>#6772e5</footer_hover_text>
    <footer_text>#414b56</footer_text>
    <footer_text_normal></footer_text_normal>
    <footer_title_font>22px</footer_title_font>
    <footer_title_text>#414b56</footer_title_text>
    <footer_top_background>#F6F7F9</footer_top_background>
  </variables>
  <layout_xml>&lt;body&gt;&#xD;
&lt;referenceBlock name="footer_links" remove="true" /&gt;&#xD;
&lt;referenceBlock name="copyright" remove="true" /&gt;&#xD;
&lt;referenceBlock name="report.bugs" remove="true" /&gt;&#xD;
&lt;referenceBlock name="form.subscribe" remove="true" /&gt;&#xD;
&lt;move element="theme-layout-footer" destination="footer-container" /&gt;&#xD;
&lt;/body&gt;</layout_xml>
  <custom_variables/>
  <custom_fields>
    <version>5f98c8ffe7f35</version>
  </custom_fields>
  <parent></parent>
  <content>&lt;div class="footer-style-01 text-center"&gt;&#xD;
&lt;div class="footer content"&gt;&#xD;
&lt;div class="footer-content-top"&gt;&#xD;
&lt;div class="row"&gt;&#xD;
&lt;div class="col-xs-24 col-sm-12 col-sm-push-6"&gt;&#xD;
&lt;div class="newsletter"&gt;&#xD;
&lt;p class="newsletter-title"&gt;SUBSCRIBE US&lt;/p&gt;&#xD;
&lt;div class="newsletter-content"&gt;{{block class="Magento\Newsletter\Block\Subscribe" name="cdz.newsletter" template="subscribe.phtml"}}&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;div class="footer-content-middle"&gt;&#xD;
&lt;div class="footer-social hover-iconsocial"&gt;&#xD;
&lt;p class="social"&gt;&#xD;
&lt;a class="spin circle" href="#"&gt;&lt;span class="fa fa-facebook"&gt;&lt;span&gt;facebook&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&#xD;
&lt;a href="#"&gt;&lt;span class="fa fa-twitter"&gt;&lt;span&gt;twitter&lt;/span&gt;&lt;/span&gt;&lt;/a&gt; &#xD;
&lt;a href="#"&gt;&lt;span class="fa fa-pinterest-p"&gt;&lt;span&gt;pinterest&lt;/span&gt;&lt;/span&gt;&lt;/a&gt; &#xD;
&lt;a href="#"&gt;&lt;span class="fa fa-google-plus"&gt;&lt;span&gt;google plus&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&#xD;
&lt;a href="#"&gt;&lt;span class="fa fa-rss"&gt;&lt;span&gt;rss&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;/p&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;div class="footer-payment"&gt;&lt;img class="img-responsive" src="{{media url="wysiwyg/codazon/footer-payment.png"}}" alt="Accept Payment" /&gt;&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;div class="footer-content-bottom"&gt;&#xD;
&lt;div class="footer-address"&gt;&#xD;
&lt;p&gt;&amp;copy; 2018 Codazon Ltd. Trademarks and brands&lt;/p&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;&#xD;
&lt;/div&gt;</content>
</config>
