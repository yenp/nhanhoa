<?xml version="1.0"?>
<config>
  <identifier>footer-style-08</identifier>
  <title>Footer Style 08</title>
  <variables>
    <footer_background>#ffffff</footer_background>
    <footer_background_file></footer_background_file>
    <footer_background_image_option></footer_background_image_option>
    <footer_background_normal>#5d666f</footer_background_normal>
    <footer_border>#000000</footer_border>
    <footer_bottom_background>#e1e1e1</footer_bottom_background>
    <footer_font>14px</footer_font>
    <footer_hover_text>#009688</footer_hover_text>
    <footer_text>#000000</footer_text>
    <footer_text_normal>#ffffff</footer_text_normal>
    <footer_title_font>30px</footer_title_font>
    <footer_title_text>#282828</footer_title_text>
    <footer_top_background>#ffffff</footer_top_background>
  </variables>
  <layout_xml>&lt;body&gt;&#xD;
&lt;referenceBlock name="footer_links" remove="true" /&gt;&#xD;
&lt;referenceBlock name="copyright" remove="true" /&gt;&#xD;
&lt;referenceBlock name="report.bugs" remove="true" /&gt;&#xD;
&lt;referenceBlock name="form.subscribe" remove="true" /&gt;&#xD;
&lt;move element="theme-layout-footer" destination="footer-container" /&gt;&#xD;
&lt;/body&gt;</layout_xml>
  <custom_variables/>
  <custom_fields>
    <version>5f98c9001cec3</version>
    <custom_variables></custom_variables>
    <custom_less_code></custom_less_code>
    <custom_css_code></custom_css_code>
  </custom_fields>
  <parent></parent>
  <content>&lt;div class="footer-style-08 text-center"&gt;&#xD;
    &lt;div class="footer content"&gt;&#xD;
        &lt;div class="footer-content-top"&gt;&#xD;
            &lt;div class="footer-social"&gt;&#xD;
                &lt;p class="social"&gt;&#xD;
                    &lt;a href="#"&gt;&#xD;
                        &lt;span class="fa fa-facebook"&gt;&#xD;
                            &lt;!-- facebook--&gt;&#xD;
                        &lt;/span&gt;&#xD;
                    &lt;/a&gt;&#xD;
                    &lt;a href="#"&gt;&#xD;
                        &lt;span class="fa fa-twitter"&gt;&#xD;
                            &lt;!-- twitter --&gt;&#xD;
                        &lt;/span&gt;&#xD;
                    &lt;/a&gt;&#xD;
                    &lt;a href="#"&gt;&#xD;
                        &lt;span class="fa fa-pinterest-p"&gt;&#xD;
                            &lt;!-- pinterest --&gt;&#xD;
                        &lt;/span&gt;&#xD;
                    &lt;/a&gt;&#xD;
                    &lt;a href="#"&gt;&#xD;
                        &lt;span class="fa fa-instagram"&gt;&#xD;
                            &lt;!-- instagram --&gt;&#xD;
                        &lt;/span&gt;&#xD;
                    &lt;/a&gt;&#xD;
                    &lt;a href="#"&gt;&#xD;
                        &lt;span class="fa fa-snapchat-ghost"&gt;&#xD;
                            &lt;!-- snapchat --&gt;&#xD;
                        &lt;/span&gt;&#xD;
