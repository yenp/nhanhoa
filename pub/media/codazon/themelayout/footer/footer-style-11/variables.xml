    <footer_background_normal>#ffffff</footer_background_normal>
    <footer_border>#ffffff</footer_border>
    <footer_bottom_background>#414b56</footer_bottom_background>
    <footer_font>14px</footer_font>
    <footer_hover_text>#ffffff</footer_hover_text>
    <footer_text>#ffffff</footer_text>
    <footer_text_normal>#131210</footer_text_normal>
    <footer_title_font>14px</footer_title_font>
    <footer_title_text>#ffffff</footer_title_text>
    <footer_top_background>transparent</footer_top_background>
